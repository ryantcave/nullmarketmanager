<Window x:Class="NullMarketManager.Views.NewTabDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NullMarketManager.Views"
        mc:Ignorable="d"
        Title="New Tab" Height="250" Width="300"
        WindowStartupLocation="CenterOwner" 
        >
    <StackPanel>
        <Label Content="Select Tab Type:" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row = "0" Grid.Column = "0"/>
        <ComboBox x:Name="TabTypeSelect" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged" Grid.Row = "0" Grid.Column = "1">
            <ComboBoxItem x:Name="Export" Content="Export"/>
        </ComboBox>
        <Button x:Name="okButton" Click="okButton_Click" IsDefault="True" Content="OK" IsEnabled="False" Grid.Row = "1" Grid.Column = "0"/>
        <Button x:Name="cancelButton" IsCancel="True" Click="cancelButton_Click" Content="Cancel" Grid.Row = "1" Grid.Column = "1"/>
        <!-- Accept or Cancel -->

    </StackPanel >
</Window>
